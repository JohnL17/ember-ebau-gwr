<SubNav
  @backRoute="project.linked-buildings"
  @backModel={{@model.projectId}}
  @backText={{t "ember-gwr.building.backToProject"}}
  @hiddenOnRoute="entrance"
  @links={{array
    (hash
      route="building.edit.form"
      models=(array @model.buildingId)
      text=(t "ember-gwr.models.building")
    )
    (hash
      route="building.edit.entrances"
      models=(array @model.buildingId)
      text=(t "ember-gwr.models.entrance" isPlural=true)
    )
  }}
/>
{{outlet}}
